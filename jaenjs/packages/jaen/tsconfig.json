{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {}
  },
  "ts-node": {
    "transpiler": "ts-node/transpilers/swc",
    "transpileOnly": true,
    "require": ["tsconfig-paths/register"],
    "compilerOptions": {
      "module": "commonjs",
      "resolveJsonModule": true
