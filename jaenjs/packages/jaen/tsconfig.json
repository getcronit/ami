{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@jaen/*": ["src/*"],
      "@jaen-admin/*": ["src/internal-plugins/admin/*"],
      "@jaen-pages/*": ["src/internal-plugins/pages/*"],
      "@jaen-notify/*": ["src/internal-plugins/notify/*"]
    }
  },
  "ts-node": {
    "transpiler": "ts-node/transpilers/swc",
    "transpileOnly": true,
    "require": ["tsconfig-paths/register"]
  },
  "include": ["**/*", "./tsconfig.json"],
  "typeRoots": ["./@types", "../../node_modules"],
  "references": [{"path": "../snek-finder"}]
}
